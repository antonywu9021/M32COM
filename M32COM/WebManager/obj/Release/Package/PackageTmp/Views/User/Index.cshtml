@model WebManager.MyPageInfo<WebModel.User>
@{
    var userType = WebManager.DAConfig.MyUser().UserType;
}
    <!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
    <html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>无标题文档</title>
        @Styles.Render("~/bundles/css")
        @Styles.Render("~/iconfont/css")
        @Scripts.Render("~/bundles/js")
    </head>
    <body>

        <div class="place">
            <span>位置：</span>
            <ul class="placeul">
               @if (userType == 0)
               {
                   <li>学员管理</li>
               }
               else
               {
                   <li>个人信息</li>
               }
            </ul>
        </div>

        <div class="rightinfo">
                <ul class="toolbar">
                    @if (userType == 0) { 
                    <li class="click" onclick="javascript:location.href='/user/edituser?id=0'"><span><img src="~/images/t01.png" /></span>添加</li>
                    }
                </ul>
            <div   id="list">
                @Html.Partial("Index_Partial", Model)
            </div>
        </div>
    </body>
</html>