@model  WebModel.PinFenTongJi 
    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0">
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <link rel="stylesheet" href="~/phone/fonts/iconfont.css" />
        <link rel="stylesheet" href="~/phone/css/font.css" />
        <link rel="stylesheet" href="~/phone/css/mui.css" />
        <link rel="stylesheet" href="~/phone/css/examdo.css" />
        <script src="~/phone/js/jquery-1.8.3.min.js"></script>
        <title>@ViewBag.Title 考试结果</title>
        <script type="text/javascript">
            pushHistory();
            window.addEventListener("popstate", function (e) {
                pushHistory();
            }, false);
            function pushHistory() {
                var state = {
                    title: "title",
                    url: "#"
                };
                window.history.pushState(state, "title", "#");
            }
        </script>
    </head>
    <body>
        <header>
            <div class="titlebar reverse">
                <a  href="/home/index">
                    <i class="iconfont">&#xe640;</i>
                </a>
                <h1>考试结果</h1>
            </div>
        </header>
        <article style="bottom: 0;">
            <ul class="xunjia-box">
                <li class="inner" style="text-align:center;">
                    <font style="color:#a0a0a0">已做题：</font>
                    <font>@Model.DaTishu 道</font>/
                    <font style="color:#a0a0a0">总题量：</font>
                    <font>@Model.SumTiShu 道</font>
                </li>
            </ul>
            <div class="details">
                <div class="detail-box">
                    <div class="detail-title" style="text-align:left;">&nbsp;&nbsp;做对试题 @Model.ZhengQueTiShu 道</div>
                    <div class="detail-wrap" style="width:80px;height:80px;float:left;text-align:center;border-right:1px solid #d5d5d5;margin-top:10px;">
                        <div style="heigh:40px;line-height:40px;">单选题</div>
                        <div style="heigh:40px;line-height:40px;">@Model.ZhengQueDanXuanTiShu</div>
                    </div>
                    <div class="detail-wrap" style="width:80px;height:80px;float:left;text-align:center;margin-top:10px;">
                        <div style="heigh:40px;line-height:40px;">判断题</div>
                        <div style="heigh:40px;line-height:40px;">@Model.ZhengQueDuoXuanTiShu</div>
                    </div>
                    <div style="clear:both"></div>
                </div>
                <div class="detail-box">
                    <div class="detail-title">总得分</div>
                    <div class="detail-wrap" style="text-align:center;padding-top:25px;padding-bottom:25px;">
                        <font style="font-size:46px;color:red;font-weight:bold;">@Model.Score</font>
                    </div>
                </div>
            </div>
        </article>
        <a class="back" href="#"></a>
        
        <script src="lib/jquery-2.1.4.js"></script>
        <script src="js/jquery-weui.js"></script>
    </body>
</html>
