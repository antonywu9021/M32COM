@model WebManager.MyPageInfo<WebModel.Department>
<table class="tablelist">
    <thead>
        <tr>
             
            <th>ID</th>
            <th>GroupName</th>
            <th>CreateTime</th>
            <th>Operation</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var mm in Model.List)
        {
            <tr>
                <td>@mm.ID</td>
                <td>@mm.Name</td>
                <td>@mm.CreateTime.ToString("yyyy-MM-dd HH:mm:ss")</td>
                <td>
                        <a href="/Department/EditDepartment?id=@mm.ID" class="tablelink" title="Edit"><i class="iconfont">&#xf014f;</i>Edit</a>
                        <a onclick="$.del('/Department/DelDepartment?id=@mm.ID','/Department/Index')" class="tablelink" title="Delete"><i class="iconfont">&#xf029a;</i>Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
@Html.PageLinks(Model.Page, m => "$.getpage('" + Model.Page.ReturnUrl + "','','#list')")
<script type="text/javascript">
$('.tablelist tbody tr:odd').addClass('odd');
</script>



