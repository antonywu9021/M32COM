@model WebManager.MyPageInfo<WebModel.User>
@{
    var userType = WebManager.DAConfig.MyUser().UserType;
}
<table class="tablelist">
    <thead>
        <tr>
            
            <th>ID<i class="sort"><img src="~/images/px.gif" /></i></th>
            <th>Match</th>
            <th>Group</th>
            <th>UserName<i class="sort"><img src="~/images/px.gif" /></i></th>
            <th>TrueName</th>
            <th>Role</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Age</th>
            <th>CreateTime</th>
            <th>Operation</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var mm in Model.List)
        {
            <tr>
                <td>@mm.ID</td>
                <td>@mm.SaiShiName</td>
                <td>@mm.DepartmentName</td>
                <td>@mm.UserName</td>
                <td>@mm.TrueName</td>
                <td>@mm.UserTypeName</td>
                <td>@mm.Phone</td>
                <td>@mm.Email</td>
                <td>@mm.Age</td>
                <td>@mm.CreateTime.ToString("yyyy-MM-dd HH:mm:ss")</td>
                <td>
                         <a href="/user/edituser?id=@mm.ID" class="tablelink" title="Edit"><i class="iconfont">&#xf014f;</i>Edit</a>
                         <a onclick="$.del('/user/deluser?id=@mm.ID','/user/index')" class="tablelink" title="Delete"><i class="iconfont">&#xf029a;</i>Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
@Html.PageLinks(Model.Page, m => "$.getpage('" + Model.Page.ReturnUrl + "','','#list')")
<script type="text/javascript">
    $('.tablelist tbody tr:odd').addClass('odd');
</script>


   
